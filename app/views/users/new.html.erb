<%= form_for @user do |f| %>
  <%= f.label :first__name %>
  <%= f.text_field :first_name %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit %>
<% end %>

<p><%= link_to "Just show me the parks!", parks_path %></p>
