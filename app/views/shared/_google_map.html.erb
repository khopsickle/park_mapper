<% coords = coord_maker(park.location_coordinates) %>

<div id="map"></div>
<script>
  function initMap() {
    var uluru = {lat: <%= coords[1].to_f %>, lng: <%= coords[0].to_f %>};
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 13,
      center: uluru
    });
    var marker = new google.maps.Marker({
      position: uluru,
      map: map
    });
  }
</script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=<%= Rails.application.secrets.google_api_key %>&callback=initMap"></script>
